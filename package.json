{
	"name": "astro",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"astro": "astro",
		"lint": "eslint . --ext .ts,.js,.astro --format=codeframe",
		"lint-fix": "eslint . --ext .ts,.js,.astro --fix --format=codeframe",
		"format": "prettier -w --cache . && prettier -w --cache **/*.astro",
		"check": "astro check",
		"sync": "astro sync",
		"prepush": "npm run lint-fix && npm run format",
		"prepare": "husky install"
	},
	"dependencies": {
		"@astrojs/tailwind": "^5.0.0",
		"astro": "^3.1.2",
		"tailwindcss": "^3.3.3"
	},
	"devDependencies": {
		"@astrojs/check": "^0.2.0",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-formatter-codeframe": "^7.32.1",
		"eslint-plugin-astro": "^0.29.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^5.0.0",
		"husky": "^8.0.3",
		"prettier": "^3.0.3",
		"prettier-plugin-astro": "^0.12.0",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"typescript": "^5.2.2"
	}
}
